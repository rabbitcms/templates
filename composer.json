{
    "name": "rabbitcms/templates",
    "description": "Mail templates for RabbitCMS",
    "require": {
        "twig/twig": "^1.24",
        "twig/extensions": "^1.3",
        "rabbitcms/modules": "^1.1",
        "rabbitcms/contracts": "^1.0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Kulyk",
            "email": "lnkvisitor.ts@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "database/migrations/"
        ],
        "psr-4": {
            "RabbitCMS\\Templates\\": "./src"
        }
    },
    "extra": {
        "module": {
            "providers": "RabbitCMS\\Templates\\Providers\\TemplatesModuleProvider",
            "system": true
        }
    }
}
